package org.jlibrary.core.search;

/**
 * @author Martín Pérez
 *
 * This exception will be launched when some error happens while
 * searching or indexing files
 */
public class SearchException extends Exception {

	static final long serialVersionUID = 1957051902984058534L;
	
	/**
	 * 
	 */
	public SearchException() {
		super();
	}

	/**
	 * @param arg0
	 */
	public SearchException(String arg0) {
		super(arg0);
	}

	/**
	 * @param arg0
	 * @param arg1
	 */
	public SearchException(String arg0, Throwable arg1) {
		super(arg0, arg1);
	}

	/**
	 * @param arg0
	 */
	public SearchException(Throwable arg0) {
		super(arg0);
	}

}
